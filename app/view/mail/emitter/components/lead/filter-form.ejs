<form id="lead-filter-form" class="box b1 container ground radius-5 margin-top-5">
  <input type="text" name="id" class="box b6 lucida-grande bold border padding-5 margin-top-5 center nofocus" placeholder="Id">
  <input type="text" name="name" class="box b2 lucida-grande bold border padding-5 margin-top-5 center nofocus" placeholder="Nome do cliente" autocomplete="off">
  <select name="status" class="box b3 lucida-grande bold border padding-4 margin-top-5 center nofocus">
    <option value="" selected>Status</option>
    <option value="Ag. contato">Ag. contato</option>
  </select>
  <input type="text" name="email" class="box b2-3 lucida-grande bold border padding-5 margin-top-5 center nofocus" placeholder="E-mail" autocomplete="off">
  <input type="text" name="phone" class="box b3 lucida-grande bold border padding-5 margin-top-5 center nofocus" placeholder="Telefone" autocomplete="off">
  <input type="date" name="periodStart" class="box b5-11 lucida-grande bold border padding-4 margin-top-5 center nofocus" autocomplete="off">
  <input type="date" name="periodEnd" class="box b5-11 lucida-grande bold border padding-4 margin-top-5 center nofocus" autocomplete="off">
  <!-- <input type="submit" name="submit" class="box b3 lucida-grande bold box-hover border-lg-st radius-5 padding-5 margin-top-5 pointer" value="Buscar"> -->
  <button type="submit" name="submit" class="mobile-box b11 noborder margin-top-5 center"><img class="size-25 icon center" src="/images/icon/search.png"></button>
</form>

<div id="lead-filter-box" class="box b1 container ground radius-5 margin-top-5">
	<div id="lead-filter-div" class="box b1 container height-300 scroll-y"></div>
</div>