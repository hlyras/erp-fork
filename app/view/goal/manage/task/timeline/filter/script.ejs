<script>
  async function timelineFilter(timeline, box) {
    let timelines = await API.response(GoalTaskTimeline.filter, timeline, box);
    if (!timelines) { return false; }

    let timeline_box = lib.element.create("div", { class: "box b1 container" });

    timelines.forEach(timeline => {
      let timeline_div = lib.element.create("div", { class: "box b1 container padding-5 margin-top-5" });
      timeline_div.append(lib.element.create("div", { class: "box b1 em07" }, `Descrição da ação - ${timeline.user_name} - ${lib.timestampToDate(timeline.datetime)}`));
      timeline_div.append(lib.element.create("div", { class: "box b1" }, timeline.description));
      timeline_box.append(timeline_div);
    });

    return timeline_box;
  };
</script>