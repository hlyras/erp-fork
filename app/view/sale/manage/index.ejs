<html>
<head>
  <% include ./../../partials/head %>
</head>
<body>
  <% include ./../../partials/header %>
  <% include ./../../partials/nav %>
  <section class="container padding-5">
    <div class="box b1 container ground border radius-5 padding-10">
    <div class="mobile-box b11-12"></div>
    <div class="mobile-box b12 container"><img src="/images/icon/close.png" class="noselect size-20 center icon transition-02-02 pointer" onclick="lib.displayDiv('filter-status-box', this, '/images/icon/down-arrow.png', '/images/icon/close.png')"></div>
    <% include ./components/filter-status %>
    </div>
  </section>

  <section class="container padding-5">
    <div class="box b1 container ground border padding-10 radius-5">
    <div class="mobile-box b11-12"></div>
    <div class="mobile-box b12 container"><img src="/images/icon/down-arrow.png" class="noselect size-20 center icon transition-02-02 pointer" onclick="lib.displayDiv('sale-filter-form', this, '/images/icon/down-arrow.png', '/images/icon/close.png')"></div>
    <% include ./components/filter-form %>
    </div>
  </section>

  <section class="container">
    <% include ./components/filter-box %>
    <div id="sale-show-box" class="box b1 container ground border radius-5 margin-top-10 margin-bottom-15"></div>
  </section>

  <section class="container margin-top-10 h-center radius-5">
    <select id="sale-category-select" class="box b3 lucida-grande bold border-st radius-5 padding-10 margin-bottom-100 hide-disabled pointer" style="display: none;">
    <option value="" selected disabled>Selecione a categoria da venda</option>
    <option value="3">Representantes</option>
    <option value="2">Atacado</option>
    <option value="1">Varejo</option>
    </select>
    <% include ./components/edit-box %>
  </section>

  <% include ./../../partials/loader %>
</body>
<footer>
  <% include ./../../partials/footer %>
  <% include ./../partial %>

  <script src="/javascripts/scripts/product/model/main.js"></script>
  <script src="/javascripts/scripts/customer/model/main.js"></script>
  <script src="/javascripts/scripts/customer/model/address.js"></script>
  <script src="/javascripts/scripts/product/model/package.js"></script>

  <script src="/javascripts/scripts/sale/model/customer.js"></script>
  <script src="/javascripts/scripts/sale/model/product.js"></script>
  <script src="/javascripts/scripts/sale/model/package.js"></script>

  <script src="/javascripts/scripts/sale/view/customer.js"></script>
  <script src="/javascripts/scripts/sale/controller/customer.js"></script>

  <script src="/javascripts/scripts/sale/view/product.js"></script>
  <script src="/javascripts/scripts/sale/controller/product.js"></script>

  <script src="/javascripts/scripts/sale/view/package.js"></script>
  <script src="/javascripts/scripts/sale/controller/package.js"></script>

  <script>
    Sale.controller.filter.submit.click();
  </script>
</footer>
