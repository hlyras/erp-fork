<form id="sale-filter-form" class="box b1 container ground padding-10 margin-top-10 shadow-hover">
  <input type="text" name="customer_name" placeholder="Nome" class="box b3 input-generic center margin-top-5"
    autocomplete="off">
  <input type="text" name="customer_cnpj" placeholder="CNPJ" class="box b3 input-generic center margin-top-5"
    autocomplete="off">
  <select name="status" class="box b3 input-generic margin-top-5 hide-disabled">
    <option value="Ag. pagamento">Ag. pagamento</option>
    <option value="Ag. pagamento 2/2">Ag. pagamento 2/2</option>
    <option value="Ag. boletos">Ag. boletos</option>
    <option value="Ag. cartão de crédito">Ag. cartão de crédito</option>
  </select>
  <input type="date" name="periodStart" class="box b3 input-generic center margin-top-5" autocomplete="off">
  <input type="date" name="periodEnd" class="box b3 input-generic center margin-top-5" autocomplete="off">
  <input type="submit" name="submit" class="box b3 submit-generic box-hover center margin-top-5 bold" value="Filtrar">
</form>