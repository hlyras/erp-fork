<form id="feedstock-filter-form" class="box b1 container ground border radius-5 padding-10 margin-top-10">
  <div class="box b1 underline center">Buscar Matéria Prima</div>
  <input type="text" name="code" class="box b5 input-generic margin-top-5 center" autocomplete="off"
    placeholder="Código">
  <input type="text" name="name" class="box b2-5 input-generic margin-top-5 center" autocomplete="off"
    placeholder="Nome do Produto">
  <select name="color_id" class="box b5 input-generic margin-top-5 center">
    <option value="">Cor</option>
    <% for(let i in colors){ %>
    <option value="<%= colors[i].id %>"><%= colors[i].name %></option>
    <% } %>
  </select>
  <input type="submit" name="submit" class="box b5 submit-generic margin-top-5 center" value="Buscar">
</form>

<div id="feedstock-filter-box" class="box a1 container" style="display:none">
  <div id="feedstock-filter-div" class="box a1 container"></div>
  <%- include('./../../../../../partials/carousel.ejs') %>
</div>

<%- include('./../report/box.ejs') %>