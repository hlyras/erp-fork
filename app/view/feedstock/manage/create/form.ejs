<form id="feedstock-create-form" class="box b1 container ground border radius-5 padding-10 margin-top-10">
  <div class="box b1 underline center">Cadastrar Matéria Prima</div>
  <input type="hidden" name="id" value="" autocomplete="off">

  <input type="number" name="code" min="1" max="9999" class="box b4 input-generic center margin-top-5"
    placeholder="Código" autocomplete="off">

  <input type="text" name="name" class="box b3-4 input-generic center margin-top-5" placeholder="Nome da matéria-prima"
    autocomplete="off" maxlength="20">

  <select name="color_id" class="box b3 input-generic center margin-top-5">
    <option value="">Cor</option>
    <% for(i in colors){ %>
    <option value="<%= colors[i].id %>"><%= colors[i].name %></option>
    <% } %>
  </select>

  <input type="number" name="unit" class="box b3 input-generic margin-top-5 center" placeholder="Quantidade do pacote"
    min="1" max="99999" autocomplete="off">

  <select name="uom" class="box b3 input-generic center margin-top-5">
    <option value="">Un de Medida</option>
    <option value="cm">Centímetros</option>
    <option value="un">Unidade</option>
  </select>

  <select name="supplier_id" class="box b2 input-generic center margin-top-5 hide-disabled">
    <option value="" selected disabled>Principal fornecedor</option>
    <% for(let i in suppliers) { %>
    <option value="<%= suppliers[i].id %>"><%= suppliers[i].brand || suppliers[i].name %></option>
    <% } %>
  </select>

  <input type="submit" name="submit" class="box b2 submit-generic margin-top-5 center" value="Cadastrar">
</form>