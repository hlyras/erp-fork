<form id="user-create-form" class="box b2 container ground border radius-5 padding-5 h-center">
  <div class="box b1 underline georgia bold margin-top-5 center">Cadastrar novo colaborador</div>
  <input type="hidden" name="id" value="">
  <input type="text" name="name" class="box b3-5 border-bottom-lg-st padding-10 margin-top-5 center"
    placeholder="Nome completo" role="presentation" autocomplete="off">
  <input type="text" name="username" class="box b2-5 border-bottom-lg-st padding-10 margin-top-5 center"
    placeholder="Usuário" role="presentation" autocomplete="off">
  <input type="text" name="password" class="box b2-5 border-bottom-lg-st padding-10 margin-top-5 center"
    placeholder="Senha" role="presentation" autocomplete="off">
  <input type="text" name="access" class="box b5 border-bottom-lg-st padding-10 margin-top-5 center"
    placeholder="Acesso" role="presentation" autocomplete="off">
  <input type="text" name="pass" class="box b2-5 border-bottom-lg-st padding-10 margin-top-5 center"
    placeholder="'Pass' de 4 dígitos" role="presentation" autocomplete="off">
  <input type="submit" name="submit"
    class="box b2-3 btn-act bold noborder radius-5 padding-10 margin-top-5 center pointer" value="Cadastrar">
</form>