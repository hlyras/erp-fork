<form id="sale-filter-form" class="box b1 container ground padding-10 margin-top-10 shadow-hover">
  <div class="box b1 underline center bold">Relat√≥rio de Embalo</div>
  <input type="date" name="period_start" class="box b4 input-generic center margin-top-5" autocomplete="off">
  <input type="date" name="period_end" class="box b4 input-generic center margin-top-5" autocomplete="off">
  <select name="packment-user-id" class="box b4 input-generic center margin-top-5" autocomplete="off">
    <option value="">Embalador</option>
    <% for(let i in users){ %>
    <option value="<%= users[i].id %>"><%= users[i].name %></option>
    <% } %>
  </select>
  <input type="submit" name="submit" class="box b4 submit-generic box-hover center margin-top-5 bold" value="Filtrar">
</form>