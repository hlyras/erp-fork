<script>
  document.getElementById("catalog-create-form").addEventListener("submit", async e => {
    e.preventDefault();

    const catalog = {
      id: e.target.id.value,
      name: e.target.name.value,
      path: e.target.path.value
    };

    console.log(catalog);

    const response = await API.response(Product.catalog.create, catalog);
    if (!response) { return false; }

    document.getElementById("catalog-filter-form").submit.click();
  });
</script>