<form id="catalog-create-form" class="box b2 container ground border radius-5 padding-5 margin-top-5">
  <div class="box b1 underline lucida-grande bold margin-top-5 center">Novo catálogo</div>
  <input type="hidden" name="id">
  <input type="text" name="name" class="box b2 input-generic lucida-grande bold margin-top-5 center"
    placeholder="Nome do catálogo" role="presentation" autocomplete="off">
  <input type="text" name="path" class="box b4 input-generic lucida-grande bold margin-top-5 center"
    placeholder="/diretorio" role="presentation" autocomplete="off">
  <input type="submit" name="submit" class="box b4 submit-generic margin-top-5" value="Cadastrar">
</form>