<div class="box b1 container border radius-5 padding-5 margin-top-5">
	<div class="mobile-box b11-12 lucida-grande bold center">Nova conferência</div>
	<div class="mobile-box b12 container center"><img id="create-display-icon" src="/images/icon/increase.png" class="noselect size-25 center icon transition-02-02 pointer" onclick="lib.displayDiv('conference-create-form', this, '/images/icon/increase.png', '/images/icon/up-arrow.png')"></div>

	<form id="conference-create-form" class="box b1 container h-center" style="display:none;">
		<input type="hidden" name="id" value="">
		<input type="hidden" name="product_id" value="">
		<input type="text" name="description" class="box b1 lucida-grande bold radius-5 border-st padding-5 margin-top-5 nofocus" placeholder="Descrição da conferência" autocomplete="off">
		<input type="text" name="video_url" class="box b1 lucida-grande bold radius-5 border-st padding-5 margin-top-5 nofocus" placeholder="URL do Vídeo de conferência" autocomplete="off">
		<input type="submit" name="submit" class="box b1 lucida-grande bold border-st box-hover radius-5 padding-5 margin-top-5 pointer" value="Criar conferência">
	</form>
</div>