<form id="activity-filter-form" class="box b2-3 container ground border radius-5 padding-10 margin-top-10">
  <div class="box b1 underline lucida-grande bold center bold">Atividade de clientes</div>
  <input type="text" name="customer_id" class="box b4 lucida-grande bold input-generic center margin-top-5" placeholder="Id do cliente" autocomplete="off">
  <input type="text" name="customer_name" class="box b3-4 lucida-grande bold input-generic center margin-top-5" placeholder="Nome do cliente" autocomplete="off">

  <input type="datetime-local" name="period_start" class="box b3-8 lucida-grande bold input-generic center margin-top-5" autocomplete="off">
  <input type="datetime-local" name="period_end" class="box b3-8 lucida-grande bold input-generic center margin-top-5" autocomplete="off">
  
  <input type="submit" name="submit" class="box b4 lucida-grande submit-generic box-hover center margin-top-5 bold" value="Atualizar">
</form>

<div id="activity-filter-box" class="box a1 container" style="display:none">
  <div id="activity-filter-div" class="box a1 container"></div>
  <%- include('./../../../../../partials/carousel.ejs') %>
</div>