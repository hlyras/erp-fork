<form id="leadlp-filter-form" class="box b2-3 container ground border radius-5 padding-5 margin-top-5">
  <input type="text" name="name" class="box b3 lucida-grande bold border radius-5 padding-5 margin-top-5 center" placeholder="Nome do cliente" autocomplete="off">
  <input type="text" name="email" class="box b3 lucida-grande bold border radius-5 padding-5 margin-top-5 center" placeholder="E-mail">
  <select name="status" class="box b3 lucida-grande bold border radius-5 padding-5 margin-top-5 center hide-disabled">
    <option value="" disabled selected>Status</option>
    <option value="Ag. contato">Ag. contato</option>
  </select>
  <input type="date" name="periodStart" class="box b3 input-generic lucida-grande bold center margin-top-5" autocomplete="off">
  <input type="date" name="periodEnd" class="box b3 input-generic lucida-grande bold center margin-top-5" autocomplete="off">
  <input type="submit" name="submit" class="box b3 lucida-grande bold box-hover border-lg-st radius-5 padding-5 margin-top-5 pointer" value="Buscar">
</form>