<html>
<head>
  <% include ./../../partials/head %>
</head>
<body>
  <% include ./../../partials/header %>
  <% include ./../../partials/nav %>
  <section class="container h-center">
    <form id="leadlp-filter-form" class="box b2-3 container ground border radius-5 padding-5 margin-top-5">
      <input type="text" name="name" class="box b4 lucida-grande bold border radius-5 padding-5 margin-top-5 center" placeholder="Nome do cliente" autocomplete="off">
      <input type="text" name="email" class="box b4 lucida-grande bold border radius-5 padding-5 margin-top-5 center" placeholder="E-mail">
      <select name="status" class="box b4 lucida-grande bold border radius-5 padding-5 margin-top-5 center hide-disabled">
        <option value="" disabled selected>Status</option>
        <option value="Ag. contato">Ag. contato</option>
      </select>
      <input type="submit" name="submit" class="box b4 lucida-grande bold box-hover border-lg-st radius-5 padding-5 margin-top-5 pointer" value="Buscar">
    </form>

    <div id="leadlp-filter-box" class="box b2-3 container ground border radius-5 padding-5 margin-top-5">
      <div id="leadlp-filter-div" class="box b1 container ground border radius-5 padding-5">
        <% include ./../../partials/carousel %>
      </div>
    </div>
  </section>
  <% include ./../../partials/loader %>
</body>
<footer>
  <% include ./../../partials/footer %>
</footer>
</html>